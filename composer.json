{
	"name": "ublaboo/aws-sdk-nette-extension",
	"type": "library",
	"description": "A Nette extension for the AWS SDK for PHP http://aws.amazon.com/sdkforphp/",
	"keywords": ["aws", "sdk", "amazon", "php", "nette", "extension", "di"],
	"homepage": "http://ublaboo.paveljanda.com/aws-sdk-nette-extension",
	"license": ["MIT"],
	"support": {
		"issues": "https://github.com/ublaboo/aws-sdk-nette-extension/issues"
	},
	"authors": [
		{
			"name": "Pavel Janda",
			"homepage": "http://paveljanda.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Ublaboo\\AwsSdkNetteExtension\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ublaboo\\AwsSdkNetteExtension\\Tests\\": "tests"
		}
	},
	"require": {
		"nette/di"        : "^2.3|^2.4",
		"aws/aws-sdk-php" : "~3.0"
	},
	"require-dev": {
		"nette/tester"    : "~1.6.1",
		"mockery/mockery" : "~0.9"
	}
}
